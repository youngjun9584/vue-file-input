{
  "name": "vue-file-input",
  "version": "0.2.1",
  "description": "파일 업로드시 해당 파일리스트를 보여주는 vue component 입니다., 기능은 갯수 제한, 크기제한 할수 있도록 만들었음",
  "readmeFilename": "README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/youngjun9584/vue-file-input.git"
  },
  "keywords": [
    "vue",
    "vue-upload",
    "vue-upload-component",
    "vue-file-upload-component",
    "vue-progress",
    "bootstrap4",
    "file-upload",
    "uploader",
    "file-uploader",
    "image-uploader",
    "file-picker",
    "file",
    "upload",
    "input",
    "multi"
  ],
  "license": "MIT",
  "main": "dist/vue-file-input.ssr.js",
  "browser": "dist/vue-file-input.esm.js",
  "module": "dist/vue-file-input.esm.js",
  "unpkg": "dist/vue-file-input.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "sideEffects": false,
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.js",
    "prebuild": "rimraf ./dist",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "dependencies": {
    "bootstrap": "^4.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "cross-env": "^7.0.3",
    "minimist": "^1.2.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.52.8",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^5.1.9",
    "vue": "^2.6.14",
    "vue-template-compiler": "^2.6.14"
  },
  "peerDependencies": {
    "vue": "^2.6.14"
  },
  "engines": {
    "node": ">=12"
  }
}
